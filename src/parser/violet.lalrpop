use std::str::FromStr;
use super::super::ast::{Mod};

grammar;

pub Mod : Mod = {
    "(" "module" <n:Id> ")" => Mod { name: n, export_list: vec![] }
}

pub Term: i32 = {
    <n:Num> => n,
    "(" <t:Term> ")" => t,
};

Num: i32 = <s:r"[0-9]+"> => i32::from_str(s).unwrap();
Id: String = <s:r"[a-zA-Z_%!*+=?]+"> => s.into();
