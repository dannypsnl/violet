module Test

data Bool
| true
| false

data Nat
| zero
| suc Nat

def id {A : U} (x : A) : A => x

def zero? (n : Nat) : Bool => elim n
| zero => true
| suc _ => false

def plus (n m : Nat) : Nat => elim n, m
| zero, m => m
| suc n, m => id $ suc $ plus n m

def not (b : Bool) : Bool => elim b
| true => id false
| false => true

def NoB (b : Bool) : U => elim b
| true => id Nat
| false => Bool

def main : NoB true => id $ plus (suc $ suc zero) (suc zero)
